<!DOCTYPE html>
<html>
  <head>
    <title>My Dev Skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>My Dev Skills</h1>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%= t.id %>"><%= t.name %></a>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
